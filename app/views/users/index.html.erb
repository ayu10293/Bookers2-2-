<div class="container px-5 px-sm-0">
<div class="row">
<%= render 'books/list', book: @book, user: @user %>
<div class="col-md-8 offset-md-1">
<%= flash[:notice] %>
<h1>Users</h1>
<table class="table">
    <thead>
      <th>image</th><th>name</th>
    </thead>
    <tbody>
    <% @users.each do |user| %>

    <tr>
      <td><%= image_tag user.get_profile_image(80,80) %></td>
      <td><%= user.name %></td>
      <td><%= link_to user_path(user.id) do %>
      Show
      <% end %>
      </td>
    </tr>
    <% end %>
    </tbody>
  </table>
</div>
</div>
</div>